{
    "name": "yfridelance/php-ssh",
    "description": "SSH Client",
    "keywords": [
        "framework",
        "ssh",
        "php",
        "contao"
    ],
    "type": "contao-module",
    "homepage": "http://fridelance.ch/",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Yves Fridelance",
            "email": "yves@fridelance.ch",
            "homepage": "http://www.fridelance.ch",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/yfridelance/php-ssh/issues",
        "source": "https://github.com/yfridelance/php-ssh"
    },
    "require": {
        "php": ">=5.3",
        "contao/core": ">=2.11,<4.0",
        "contao-community-alliance/composer-plugin": "~2.0"
    },
    "require-dev": {
        "contao-community-alliance/coding-standard": "~1.0"
    },
    "conflict": {
        "contao/core": "3.0.*"
    },
    "autoload": {
        "classmap": [
            "src/Ssh/",
            "src/Ssh/Authentication/"
        ]
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/php-ssh"
            }
        }
    }
}
